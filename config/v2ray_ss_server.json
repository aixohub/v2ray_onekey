{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "warning"
  },
  "routing": {
      "domainStrategy": "AsIs",
      "rules": [
          {
              "type": "field",
              "ip": [
                  "geoip:private"
              ],
              "outboundTag": "block"
          }
      ]
  },
  "inbounds": [
      {
          "listen": "127.0.0.1",
          "port": 10000,
          "protocol": "shadowsocks",
          "settings": {
              "method": "chacha20-ietf-poly1305",
              "password": "{{ password }}"
          },
          "streamSettings": {
              "network": "ws",
              "wsSettings": {
                "path": "xx"
            }
          }
      }
  ],
  "outbounds": [
      {
          "protocol": "freedom",
          "tag": "direct"
      },
      {
          "protocol": "blackhole",
          "tag": "block"
      }
  ]
}